{
    "name": "coc-phpactor",
    "version": "1.0.0",
    "description": "Phpactor PHP Language Server extension for coc.nvim",
    "main": "lib/index.js",
    "publisher": "dantleech",
    "engines": {
        "coc": "^0.0.66"
    },
    "keywords": [
        "coc.nvim",
        "php",
        "phpactor"
    ],
    "scripts": {
        "clean": "rimraf lib",
        "build": "tsc -p tsconfig.json",
        "prepare": "yarn clean && yarn build"
    },
    "activationEvents": [
        "onLanguage:php"
    ],
    "contributes": {
        "configuration": {
            "title": "phpactor config",
            "properties": {
                "phpactor.path": {
                    "type": "string",
                    "default": "",
                    "description": "phpactor server path"
                },
                "phpactor.enable": {
                    "type": "boolean",
                    "default": true,
                    "description": "enable phpactor language server"
                }
            }
        }
    },
    "author": "daniel@dantleech.com",
    "license": "MIT",
    "devDependencies": {
        "@chemzqm/tsconfig": "^0.0.3",
        "@chemzqm/tslint-config": "^1.0.18",
        "@types/node": "^11.13.10",
        "coc.nvim": "^0.0.77",
        "rimraf": "^2.6.3",
        "ts-loader": "^6.0.3",
        "tslint": "^5.16.0",
        "typescript": "^3.4.4",
        "vscode-languageserver-protocol": "^3.15.0-next.4",
        "webpack": "^4.34.0",
        "webpack-cli": "^3.3.4"
    },
    "dependencies": {
    }
}
